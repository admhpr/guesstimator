{
  "name": "guesstimator",
  "description": "Take the guess work out of estimating, ( kinda ).", 
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "./images/icon.png", 
    "default_popup": "./pages/popup.html",
    "default_tab": "./pages/tab.html",
    "default_option": "./pages/options.html",
    "default_title": "Stay on track."
  },
  "permissions": ["activeTab", "http://*/", "storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
